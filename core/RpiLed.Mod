MODULE* RpiLed;
IMPORT SYSTEM;
  PROCEDURE Blink;
     VAR i,j: INTEGER;
  BEGIN j:=1;
    SYSTEM.PUT(20200000H + 4, 40000H);
    WHILE j # 0 DO
      SYSTEM.PUT(20200000H+ 40,10000H);
      i:=3F0000H;  WHILE i # 0 DO  DEC(i) END ; 
      SYSTEM.PUT(20200000H+ 28,10000H);
      i:=3F0000H;  WHILE i # 0 DO  DEC(i) END ; 
    END
  END Blink;

BEGIN
  Blink
END RpiLed.
